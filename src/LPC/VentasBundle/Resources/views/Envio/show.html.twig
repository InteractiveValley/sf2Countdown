{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Envio</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Venta</th>
                <td>{{ entity.venta }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ entity.direccion }}</td>
            </tr>
            <tr>
                <th>Numeroguia</th>
                <td>{{ entity.numeroGuia }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('envio') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('envio_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
