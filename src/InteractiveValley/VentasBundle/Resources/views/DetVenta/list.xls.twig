<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Venta</th>
            <th>Producto</th>
            <th>Cantidad</th>
            <th>Precio</th>
        </tr>
    </thead>
    <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ entity.venta.id }}</td>
                <td>{{ entity.producto.nombre }}</td>
                <td>{{ entity.cantidad }}</td>
                <td>{{ entity.precio }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>